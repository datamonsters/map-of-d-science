{
    "compilerOptions": {
        "allowUnreachableCode":true,
        "target": "es5",
        "module": "system",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports":true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": true,
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "app/*": [
                "./app/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "scripts",
        "libs",
        "assets"
    ],
    "files": [
        "app/definitions/index.d.ts",
        "app/definitions/sigma.d.ts",
        "app/definitions/jquerry.d.ts",
        "app/definitions/ramda/index.d.ts"
    ]
}